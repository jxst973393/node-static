<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
    <link rel="stylesheet" href="./css/bootstrap.css">
    <script type="text/javascript" src="./js/jquery-3.1.1.min.js"></script>
</head>
<body>
<div class=container>
    <div class="row">
        <div class="col-md-6">
            <form role="form" action="/fileupload" method="post" enctype="multipart/form-data">
                <div class="form-group">
                    <label>请选择文件:</label>
                    <input type="file" class="form-control" name="uploadFile"/>
                </div>
                <div class="form-group">
                    <input type="submit" class="btn btn-success" value="上传"/>
                </div>
            </form>
        </div>
        <div class="col-md-6"></div>
        <div class="row">
            <% for(let index in data){%>
            <div class="col-md-3">
                <div class="thumbnail">
                    <img src="upload/<%= data[index].filename%>" style="width:200px;height: 200px;">
                    <div class="caption">
                        <h4><%= data[index].type%></h4>
                        <p>
                            <a href="/list/<%= data[index].id %>" class="btn btn-primary" role="button">详细页面</a>
                        </p>
                    </div>
                </div>
            </div>
            <% } %>
        </div>
    </div>
</div>
</body>
</html>